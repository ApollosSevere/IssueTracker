    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "fortawesome": "0.0.1-security",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.9.50",
    "lodash": "^4.17.4",
    "moment": "^2.29.2",
    "morgan": "^1.9.1",
    "pg": "^8.5.1",
    "react-chartjs-2": "2.11.1",
    "react-datepicker": "^4.6.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-google-charts": "^3.0.15",
    "react-loading-icons": "^1.0.8",
    "react-select": "^5.2.2",
    "react-widgets": "^5.6.1",
    "reactstrap": "^8.9.0",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
